{
  "apps": {
    "myApp.ios": {
      "type": "ios.app",
      "binaryPath": "e2e/Exponent.app",
    },
  },
  "devices": {
    "simulator": {
      "type": "ios.simulator",
      "device": {
        "type": "iPhone 12",
      },
    },
  },
  "configurations": {
    "ios.sim.release": {
      /* REMOVE (!) "type": "ios.simulator" */
      "device": "simulator",
      "app": "myApp.ios",
    }
  },
  "artifacts": {
    "rootDir": ".artifacts",
    "plugins": {
      "instruments": { "enabled": false },
      "log": { "enabled": true },
      "uiHierarchy": "enabled",
      "screenshot": {
        "shouldTakeAutomaticSnapshots": true,
        "keepOnlyFailedTestsArtifacts": true,
      }
    }
  },
  "test-runner": "jest",
  "runner-config": "./e2e/config.json"
}
